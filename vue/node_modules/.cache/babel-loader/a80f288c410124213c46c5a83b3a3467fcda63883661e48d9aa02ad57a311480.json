{"ast":null,"code":"export default {\n  name: \"OpenShare\",\n  data() {\n    return {\n      code: this.$route.query.code,\n      //验证码\n      shareId: this.$route.query.shareId,\n      //分享信息\n      share: {},\n      validateFailed: false //验证失败\n    };\n  },\n\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.$request.get('/share/selectById/' + this.shareId).then(res => {\n        this.share = res.data || {};\n        //判断验证码是否正确\n        if (this.share.code !== this.code) {\n          this.validateFailed = true;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","code","$route","query","shareId","share","validateFailed","created","load","methods","$request","get","then","res"],"sources":["src/views/front/OpenShare.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--分享失败-->\r\n    <div style=\"margin: 50px auto; text-align: center;\r\n    font-size: 150px; color: #666666\" v-if=\"validateFailed\">\r\n      The file no longer exists or has been deleted.\r\n    </div>\r\n    <!--分享成功-->\r\n    <div class=\"el-card\" style=\"margin: 5px auto; width: 60%\" v-else>\r\n      <div style=\"color: #666666; display: flex; align-items: center\">\r\n        Share: <strong>{{share.name}}</strong>\r\n        <i class=\"el-icon-time\" style=\"margin-left: 20px; margin-right: 5px\"></i>{{share.shareTime}}\r\n        <span style=\"margin-left: 20px\">\r\n          <i class=\"el-icon-info\" style=\"margin-right: 5px\"></i>\r\n          <span v-if=\"share.status.includes('hours')\" style=\"color: #f13b3b\">{{share.status}}</span>\r\n          <span v-else=>{{share.status}}</span>\r\n        </span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"OpenShare\",\r\n  data(){\r\n    return{\r\n      code: this.$route.query.code, //验证码\r\n      shareId: this.$route.query.shareId, //分享信息\r\n      share: {},\r\n      validateFailed: false, //验证失败\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods:{\r\n    load(){\r\n      this.$request.get('/share/selectById/'+this.shareId).then(res=>{\r\n        this.share = res.data || {}\r\n        //判断验证码是否正确\r\n        if(this.share.code !== this.code){\r\n          this.validateFailed = true\r\n        }\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAuBA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,IAAA;MAAA;MACAG,OAAA,OAAAF,MAAA,CAAAC,KAAA,CAAAC,OAAA;MAAA;MACAC,KAAA;MACAC,cAAA;IACA;EACA;;EAEAC,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IACAD,KAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,6BAAAP,OAAA,EAAAQ,IAAA,CAAAC,GAAA;QACA,KAAAR,KAAA,GAAAQ,GAAA,CAAAb,IAAA;QACA;QACA,SAAAK,KAAA,CAAAJ,IAAA,UAAAA,IAAA;UACA,KAAAK,cAAA;QACA;MACA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}