{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nexport default {\n  data() {\n    return {\n      categoryList: [{\n        \"text\": 'All Files',\n        category: 'all',\n        icon: ''\n      }, {\n        \"text\": 'Image',\n        category: 'img',\n        icon: 'el-icon-picture-outline'\n      }, {\n        \"text\": 'Video',\n        category: 'video',\n        icon: 'el-icon-video-play'\n      }, {\n        \"text\": 'Zip',\n        category: 'zip',\n        icon: 'el-icon-box'\n      }],\n      typeList: [{\n        text: 'mp3',\n        icon: 'el-icon-mp3'\n      }, {\n        text: 'mp4',\n        icon: 'el-icon-mp4'\n      }, {\n        text: 'jpg',\n        icon: 'el-icon-jpg'\n      }, {\n        text: 'jpeg',\n        icon: 'el-icon-jpeg'\n      }, {\n        text: 'png',\n        icon: 'el-icon-png'\n      }, {\n        text: 'pdf',\n        icon: 'el-icon-pdf'\n      }, {\n        text: 'docx',\n        icon: 'el-icon-docx'\n      }, {\n        text: 'txt',\n        icon: 'el-icon-text'\n      }, {\n        text: 'zip',\n        icon: 'el-icon-zip'\n      }, {\n        text: 'folder',\n        icon: 'el-icon-folder'\n      }],\n      category: this.$route.query.category,\n      tableData: [],\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      uploadUrl: '',\n      folderId: this.$route.query.folderId\n    };\n  },\n  mounted() {\n    this.load();\n    this.uploadUrl = this.$baseUrl + '/diskFiles/add?folder=No';\n    if (this.folderId) {\n      // 如果不这么判断  就会传 undefined到后台 就会报错\n      this.uploadUrl += '&folderId=' + this.folderId;\n    }\n  },\n  methods: {\n    //下载文件\n    download() {\n      window.open(url); // 文件下载\n    },\n\n    //重新命名\n    rename(row) {\n      this.$set(row, 'unSave', true);\n    },\n    //创建文件夹\n    addFolder() {\n      this.tableData.unshift({\n        name: 'New Folder',\n        type: 'folder',\n        unSave: true\n      });\n    },\n    //确认创建文件夹\n    saveFolder(row) {\n      let url = '/diskFiles/add?folder=Yes&name=' + row.name;\n      if (this.folderId) {\n        // 外层的folderId\n        url += '&folderId=' + this.folderId;\n      }\n      this.$request.post(url).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"Create Successfully\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    //取消创建文件夹\n    cancel(index) {\n      this.tableData.splice(index, 1);\n      this.load();\n    },\n    //加载数据\n    load() {\n      this.$request.get('/diskFiles/selectAll').then(res => {\n        this.tableData = res.data || [];\n        this.tableData.forEach(item => {\n          this.$set(item, 'optShow', false); // 强制设置每行  加一个 optShow属性\n        });\n      });\n    },\n\n    //处理上传成功\n    handleFileSuccess(res) {\n      if (res.code === '200') {\n        this.$message.success(\"Upload Successfully\");\n        this.load();\n      } else {\n        this.$message.error('Upload Failed');\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","categoryList","category","icon","typeList","text","$route","query","tableData","user","JSON","parse","localStorage","getItem","uploadUrl","folderId","mounted","load","$baseUrl","methods","download","window","open","url","rename","row","$set","addFolder","unshift","name","type","unSave","saveFolder","$request","post","then","res","code","$message","success","error","msg","cancel","index","splice","get","forEach","item","handleFileSuccess"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"display: flex; color: #666\">\r\n      <div style=\"width: 200px; text-align: center;  border-right: 1px solid #ddd; min-height: calc(100vh - 86px)\">\r\n        <!-- 第一部分-->\r\n        <div style=\"padding: 10px 0\">\r\n          <div class=\"category\" style=\"font-weight: bold; padding: 10px 0\" v-for=\"item in categoryList\" :key=\"item.text\" :class=\"{ 'category-active' : category === item.category}\">\r\n            <i v-if=\"item.icon\" :class=\"item.icon\" style=\"margin-right: 5px\"></i>\r\n            <span>{{ item.text }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 第二部分-->\r\n        <div style=\"border-top: 1px solid #ddd; text-align: center; padding-top: 10px\">\r\n          <div style=\"padding: 10px 0; display: flex; justify-content: center\" class=\"category\" :class=\"{ 'category-active' : category === 'share'}\">\r\n            <div style=\"width: 75px; text-align: left\">\r\n              <i class=\"el-icon-share\" style=\"margin-right: 5px\"></i>\r\n              <span style=\"font-weight: bold\">Share</span>\r\n            </div>\r\n          </div>\r\n          <div style=\"padding: 10px 0; display: flex; justify-content: center\" class=\"category\" :class=\"{ 'category-active' : category === 'trash'}\">\r\n            <div style=\"width: 75px; text-align: left\">\r\n              <i class=\"el-icon-delete\" style=\"margin-right: 5px\"></i>\r\n              <span style=\"font-weight: bold\">Trash</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div style=\"flex: 1\">\r\n        <div>\r\n          <div style=\"padding: 15px; border-bottom: 1px solid #ddd\">\r\n<!--            上传文件-->\r\n            <el-upload\r\n                style=\"display: inline-block; margin-right: 10px\"\r\n                :action=\"uploadUrl\"\r\n                :headers=\"{ token: user.token }\"\r\n                :show-file-list=\"false\"\r\n                :on-success=\"handleFileSuccess\"\r\n            >\r\n              <el-button type=\"primary\">Upload</el-button>\r\n            </el-upload>\r\n            <el-button type=\"primary\" plain @click=\"addFolder\">Create New</el-button>\r\n            <el-button type=\"danger\" plain>Delete</el-button>\r\n          </div>\r\n        </div>\r\n\r\n        <div style=\"padding: 15px\">\r\n          <span style=\"font-weight: bold\">All Files <i class=\"el-icon-arrow-right\"></i></span>\r\n        </div>\r\n\r\n        <div>\r\n          <el-table stripe :data=\"tableData\">\r\n            <el-table-column label=\"Name\">\r\n              <template v-slot=\"scope\">\r\n                <div style=\"display: flex\" @mouseenter=\"scope.row.optShow = true\" @mouseleave=\"scope.row.optShow = false\">\r\n                  <div style=\"cursor: pointer; flex: 1\">\r\n                    <i style=\"font-size:25px; color: #409EFF\" :class=\"typeList.find(v => v.text === scope.row.type)?.icon || 'el-icon-file'\"></i>\r\n                    <div v-if=\"scope.row.unSave\" style=\"display: inline-block; margin-left: 5px\" v-else>\r\n                      <el-input size=\"mini\" style=\"width: 120px; margin-right: 10px;\" v-model=\"scope.row.name\"></el-input>\r\n                      <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-check\" @click=\"saveFolder(scope.row)\"></el-button>\r\n                      <el-button type=\"danger\" size=\"mini\" icon=\"el-icon-close\" @click=\"cancel(scope.row.$index)\"></el-button>\r\n                    </div>\r\n                    <span v-else=\"scope.row.unSave\" style=\"margin-left: 5px\">{{ scope.row.name }}</span>\r\n                  </div>\r\n                  <div style=\"color: #409EFF; font-size: 13px\" v-if=\"scope.row.optShow && scope.row.id\">\r\n                    <el-tooltip content=\"Share\" effect=\"light\" :open-delay=\"1000\">\r\n                      <i class=\"el-icon-share\" style=\"margin-right: 10px; cursor: pointer\"></i>\r\n                    </el-tooltip>\r\n                    <el-tooltip content=\"Upload\" effect=\"light\" :open-delay=\"1000\">\r\n                      <i class=\"el-icon-download\" style=\"margin-right: 10px; cursor: pointer\" @click=\"download\"></i>\r\n                    </el-tooltip>\r\n<!--                    <el-tooltip content=\"Delete\" effect=\"light\" :open-delay=\"1000\">-->\r\n<!--                      <i class=\"el-icon-delete\" style=\"margin-right: 10px; cursor: pointer\"></i>-->\r\n<!--                    </el-tooltip>-->\r\n                    <el-tooltip content=\"Rename\" effect=\"light\" :open-delay=\"1000\">\r\n                      <i class=\"el-icon-rename\" style=\"margin-right: 10px; cursor: pointer\" @click=\"rename(scope.row)\"></i>\r\n                    </el-tooltip>\r\n                    <el-tooltip content=\"Copy\" effect=\"light\" :open-delay=\"1000\">\r\n                      <i class=\"el-icon-document-copy\" style=\"cursor: pointer\"></i>\r\n                    </el-tooltip>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"updateTime\" label=\"Time\"></el-table-column>\r\n            <el-table-column prop=\"size\" label=\"Size(KB)\"></el-table-column>\r\n          </el-table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      categoryList: [\r\n        {\"text\": 'All Files', category: 'all', icon: ''},\r\n        {\"text\": 'Image', category: 'img', icon: 'el-icon-picture-outline'},\r\n        {\"text\": 'Video', category: 'video', icon: 'el-icon-video-play'},\r\n        {\"text\": 'Zip', category: 'zip', icon: 'el-icon-box'},\r\n      ],\r\n      typeList: [\r\n        { text: 'mp3', icon: 'el-icon-mp3' },\r\n        { text: 'mp4', icon: 'el-icon-mp4' },\r\n        { text: 'jpg', icon: 'el-icon-jpg' },\r\n        { text: 'jpeg', icon: 'el-icon-jpeg' },\r\n        { text: 'png', icon: 'el-icon-png' },\r\n        { text: 'pdf', icon: 'el-icon-pdf' },\r\n        { text: 'docx', icon: 'el-icon-docx' },\r\n        { text: 'txt', icon: 'el-icon-text' },\r\n        { text: 'zip', icon: 'el-icon-zip' },\r\n        { text: 'folder', icon: 'el-icon-folder' },\r\n      ],\r\n      category: this.$route.query.category,\r\n      tableData: [],\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      uploadUrl: '',\r\n      folderId: this.$route.query.folderId\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.load()\r\n\r\n    this.uploadUrl = this.$baseUrl + '/diskFiles/add?folder=No'\r\n    if (this.folderId) {  // 如果不这么判断  就会传 undefined到后台 就会报错\r\n      this.uploadUrl += '&folderId=' + this.folderId\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    //下载文件\r\n    download(){\r\n      window.open(url)  // 文件下载\r\n    },\r\n    //重新命名\r\n    rename(row){\r\n      this.$set(row, 'unSave', true)\r\n    },\r\n    //创建文件夹\r\n    addFolder(){\r\n      this.tableData.unshift({name:'New Folder', type:'folder',unSave: true})\r\n    },\r\n    //确认创建文件夹\r\n    saveFolder(row){\r\n      let url = '/diskFiles/add?folder=Yes&name=' + row.name\r\n      if (this.folderId) {  // 外层的folderId\r\n        url += '&folderId=' + this.folderId\r\n      }\r\n      this.$request.post(url).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"Create Successfully\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    //取消创建文件夹\r\n    cancel(index){\r\n      this.tableData.splice(index,1)\r\n      this.load()\r\n    },\r\n    //加载数据\r\n    load() {\r\n      this.$request.get('/diskFiles/selectAll').then(res => {\r\n        this.tableData = res.data || []\r\n        this.tableData.forEach(item => {\r\n          this.$set(item, 'optShow', false)   // 强制设置每行  加一个 optShow属性\r\n        })\r\n      })\r\n    },\r\n    //处理上传成功\r\n    handleFileSuccess(res) {\r\n      if (res.code === '200') {\r\n        this.$message.success(\"Upload Successfully\")\r\n        this.load()\r\n      } else {\r\n        this.$message.error('Upload Failed')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.category {\r\n  cursor: pointer;\r\n}\r\n.category:hover {\r\n  background-color: #f8f8f8;\r\n  color: #000;\r\n}\r\n.category-active {\r\n  color: #409EFF;\r\n}\r\n</style>\r\n"],"mappings":";AAgGA;EAEAA,KAAA;IACA;MACAC,YAAA,GACA;QAAA;QAAAC,QAAA;QAAAC,IAAA;MAAA,GACA;QAAA;QAAAD,QAAA;QAAAC,IAAA;MAAA,GACA;QAAA;QAAAD,QAAA;QAAAC,IAAA;MAAA,GACA;QAAA;QAAAD,QAAA;QAAAC,IAAA;MAAA,EACA;MACAC,QAAA,GACA;QAAAC,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,GACA;QAAAE,IAAA;QAAAF,IAAA;MAAA,EACA;MACAD,QAAA,OAAAI,MAAA,CAAAC,KAAA,CAAAL,QAAA;MACAM,SAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,QAAA,OAAAT,MAAA,CAAAC,KAAA,CAAAQ;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,IAAA;IAEA,KAAAH,SAAA,QAAAI,QAAA;IACA,SAAAH,QAAA;MAAA;MACA,KAAAD,SAAA,wBAAAC,QAAA;IACA;EACA;EAEAI,OAAA;IACA;IACAC,SAAA;MACAC,MAAA,CAAAC,IAAA,CAAAC,GAAA;IACA;;IACA;IACAC,OAAAC,GAAA;MACA,KAAAC,IAAA,CAAAD,GAAA;IACA;IACA;IACAE,UAAA;MACA,KAAAnB,SAAA,CAAAoB,OAAA;QAAAC,IAAA;QAAAC,IAAA;QAAAC,MAAA;MAAA;IACA;IACA;IACAC,WAAAP,GAAA;MACA,IAAAF,GAAA,uCAAAE,GAAA,CAAAI,IAAA;MACA,SAAAd,QAAA;QAAA;QACAQ,GAAA,wBAAAR,QAAA;MACA;MACA,KAAAkB,QAAA,CAAAC,IAAA,CAAAX,GAAA,EAAAY,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAtB,IAAA;QACA;UACA,KAAAqB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IACA;IACAC,OAAAC,KAAA;MACA,KAAAnC,SAAA,CAAAoC,MAAA,CAAAD,KAAA;MACA,KAAA1B,IAAA;IACA;IACA;IACAA,KAAA;MACA,KAAAgB,QAAA,CAAAY,GAAA,yBAAAV,IAAA,CAAAC,GAAA;QACA,KAAA5B,SAAA,GAAA4B,GAAA,CAAApC,IAAA;QACA,KAAAQ,SAAA,CAAAsC,OAAA,CAAAC,IAAA;UACA,KAAArB,IAAA,CAAAqB,IAAA;QACA;MACA;IACA;;IACA;IACAC,kBAAAZ,GAAA;MACA,IAAAA,GAAA,CAAAC,IAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,KAAAtB,IAAA;MACA;QACA,KAAAqB,QAAA,CAAAE,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}