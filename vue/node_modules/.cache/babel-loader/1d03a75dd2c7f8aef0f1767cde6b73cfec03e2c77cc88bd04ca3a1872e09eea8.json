s.$message.error(res.msg);\n        }\n      });\n    }\n  },\n  //签到\n  signin() {\n    let data = {\n      userId: this.user.id\n    };\n    this.$request.post('signin/add', data).then(res => {\n      if (res.code === '200') {\n        this.$message.success('签到成功，恭喜你获得10个积分');\n      } else {\n        this.$message.error(res.msg);\n      }\n    });\n  },\n  //获取签到数据\n  getSign() {\n    this.$request.get('/signin/selectById/').then(res => {\n      if (res.code === '200') {\n        this.signData = res.data;\n      } else {\n        this.$message.error(res.msg);\n      }\n    });\n  }\n};","map":{"version":3,"names":["data","carouselData","require","recommend","rightData","type","infoRecommend","infoData","signData","user","JSON","parse","localStorage","getItem","mounted","getData","getInformation","getSign","methods","initValue","getRecommend","getRightData","url","$request","get","then","res","code","$message","error","msg","signin","userId","id","post","success"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div>\r\n    <!--轮播图-->\r\n    <el-carousel height=\"350px\">\r\n      <el-carousel-item v-for=\"item in carouselData\">\r\n        <img :src=\"item\" alt=\"\" style=\"width: 100%; height: 400px\">\r\n      </el-carousel-item>\r\n    </el-carousel>\r\n  </div>\r\n\r\n  <!--课程内容-->\r\n    <div style=\"width: 80%; margin: 30px auto;\">\r\n      <!--按钮-->\r\n      <div style=\"display: flex\">\r\n        <div style=\"flex: 1\">\r\n          <span style=\"color: #333333; font-size: 24px; font-weight: bold;\r\n          margin-right: 50px\">在线课程</span>\r\n          <el-button type=\"primary\" @click=\"initValue('VIDEO')\">视频课程</el-button>\r\n          <el-button type=\"warning\" @click=\"initValue('TEXT')\">图文课程</el-button>\r\n          <el-button type=\"success\" @click=\"initValue('SCORE')\">积分专区</el-button>\r\n        </div>\r\n        <div style=\"width: 300px\">\r\n          <el-button type=\"inf