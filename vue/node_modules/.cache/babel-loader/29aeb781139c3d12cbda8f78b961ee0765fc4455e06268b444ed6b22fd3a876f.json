{"ast":null,"code":"export default {\n  name: \"Trash\",\n  props: {\n    typeList: null\n  },\n  data() {\n    return {\n      tableData: []\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  methods: {\n    //批量删除\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning('Have no items to delete');\n        return;\n      }\n      this.$confirm('Are you sure you want to delete?', 'Delete Confirm', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/diskFiles/delete/batch', {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('Delete Successfully');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    // 加载数据\n    load() {\n      this.$request.get('/diskFiles/selectTrash').then(res => {\n        this.tableData = res.data || [];\n      });\n    },\n    mouseEnter(row) {\n      this.$set(row, 'optShow', true);\n    },\n    mouseLeave(row) {\n      this.$set(row, 'optShow', false);\n    },\n    handleSelectionChange(rows) {\n      // 当前选中的所有的行数据\n      this.ids = rows.map(v => v.id);\n    }\n  }\n};","map":{"version":3,"names":["name","props","typeList","data","tableData","mounted","load","methods","delBatch","ids","length","$message","warning","$confirm","type","then","response","$request","delete","res","code","success","error","msg","catch","get","mouseEnter","row","$set","mouseLeave","handleSelectionChange","rows","map","v","id"],"sources":["src/components/Trash.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--增删-->\r\n    <div>\r\n      <div style=\"padding: 15px; border-bottom: 1px solid #ddd\">\r\n        <el-button type=\"danger\" plain @click=\"delBatch\">Delete</el-button>\r\n      </div>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe>\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n      <el-table-colum label=\"Name\">\r\n        <template>\r\n          <div style=\"display: flex; cursor: pointer\" @mouseenter=\"mouseEnter(scope.row)\"  @mouseleave=\"mouseLeave(scope.row)\">\r\n            <div style=\" flex: 1\">\r\n              <i style=\"font-size:25px; color: #409EFF\" :class=\"typeList.find(v => v.text === scope.row.type)?.icon || 'el-icon-file'\"></i>\r\n              <span v-if=\"!scope.row.unSave\" style=\"margin-left: 5px\">{{ scope.row.name }}</span>\r\n            </div>\r\n            <div style=\"color: #409EFF; font-size: 13px\" v-if=\"scope.row.optShow && scope.row.id\">\r\n              <el-tooltip content=\"Delete\" effect=\"light\" :open-delay=\"1000\">\r\n                <i class=\"el-icon-delete\" style=\"margin-right: 10px; cursor: pointer\"></i>\r\n              </el-tooltip>\r\n              <el-tooltip content=\"Restore\" effect=\"light\" :open-delay=\"1000\">\r\n                <i class=\"el-icon-refresh\" style=\"margin-right: 10px; cursor: pointer\"></i>\r\n              </el-tooltip>\r\n            </div>\r\n          </div>\r\n        </template>\r\n      </el-table-colum>\r\n      <el-table-colum prop=\"updateTime\" label=\"Delete Time\" width=\"300\"></el-table-colum>\r\n      <el-table-colum prop=\"size\" label=\"Size(KB)\" width=\"300\"></el-table-colum>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Trash\",\r\n  props:{\r\n    typeList: null\r\n  },\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.load()\r\n  },\r\n\r\n  methods: {\r\n\r\n\r\n    //批量删除\r\n    delBatch() {\r\n      if (!this.ids.length) {\r\n        this.$message.warning('Have no items to delete')\r\n        return\r\n      }\r\n      this.$confirm('Are you sure you want to delete?', 'Delete Confirm', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/diskFiles/delete/batch', {data: this.ids}).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('Delete Successfully')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n\r\n\r\n    // 加载数据\r\n    load() {\r\n      this.$request.get('/diskFiles/selectTrash').then(res => {\r\n        this.tableData = res.data || []\r\n      })\r\n    },\r\n\r\n    mouseEnter(row) {\r\n      this.$set(row, 'optShow', true)\r\n    },\r\n    mouseLeave(row) {\r\n      this.$set(row, 'optShow', false)\r\n    },\r\n\r\n    handleSelectionChange(rows) {   // 当前选中的所有的行数据\r\n      this.ids = rows.map(v => v.id)\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAmCA;EACAA,IAAA;EACAC,KAAA;IACAC,QAAA;EACA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IAGA;IACAC,SAAA;MACA,UAAAC,GAAA,CAAAC,MAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA;MACA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAC,IAAA,CAAAC,QAAA;QACA,KAAAC,QAAA,CAAAC,MAAA;UAAAf,IAAA,OAAAM;QAAA,GAAAM,IAAA,CAAAI,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAT,QAAA,CAAAU,OAAA;YACA,KAAAf,IAAA;UACA;YACA,KAAAK,QAAA,CAAAW,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IAGA;IACAlB,KAAA;MACA,KAAAW,QAAA,CAAAQ,GAAA,2BAAAV,IAAA,CAAAI,GAAA;QACA,KAAAf,SAAA,GAAAe,GAAA,CAAAhB,IAAA;MACA;IACA;IAEAuB,WAAAC,GAAA;MACA,KAAAC,IAAA,CAAAD,GAAA;IACA;IACAE,WAAAF,GAAA;MACA,KAAAC,IAAA,CAAAD,GAAA;IACA;IAEAG,sBAAAC,IAAA;MAAA;MACA,KAAAtB,GAAA,GAAAsB,IAAA,CAAAC,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}